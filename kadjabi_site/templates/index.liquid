{% layout 'home' %}
{{ 'index.css' | asset_url | stylesheet_tag }}
<div class="hero">
  <div id="hero_animation_container">
    <canvas id="hero_canvas" width="1920" height="1300"></canvas>
  </div>
  <div id="sje_animation_container">
    <canvas id="sje_canvas" width="910" height="90"></canvas>
  </div>
  <h1>a Journey Home</h1>
  <p>Sign up to receive a free meditation video and take the first step…</p>
