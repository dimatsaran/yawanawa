<!DOCTYPE html>
<html lang="en">
<head>
    {% csrf_meta_tags %}
    <title>
        {% if page_title %}
            {{ page_title }}
        {% else %}
            {{ current_site.title }}
        {% endif %}
    </title>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    {% include "social_meta_tags" %}
    <link href="{{ 'favicon.png' | asset_url }}?v=2" rel="shortcut icon" />
    <link rel="canonical" href="{{ canonical_url }}" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    {{ 'bootstrap-480-767-grid.css' | asset_url | stylesheet_tag }}
    {{ 'fonts.css' | asset_url | stylesheet_tag }}
    {{ 'media.css' | asset_url | stylesheet_tag }}
    {{ 'style.css' | asset_url | stylesheet_tag }}
    {% if settings.font_family ==  settings.heading_font_family%}
        <link href="//fonts.googleapis.com/css?family={{ settings.font_family | replace: ' ', '+' }}:300,400,600,700,300italic,400italic,600italic,700italic" rel="stylesheet" type="text/css">
    {% else %}
        <link href="//fonts.googleapis.com/css?family={{ settings.font_family | replace: ' ', '+' }}:300,400,600,700,300italic,400italic,600italic,700italic|{{ settings.heading_font_family | replace: ' ', '+' }}:300,400,600,700,300italic,400italic,600italic,700italic" rel="stylesheet" type="text/css">
    {% endif %}
    {{ 'screen.css' | asset_url | stylesheet_tag }}
    {% content_for_header %}
    <link href="https://fonts.googleapis.com/css?family=Qwigley" rel="stylesheet">
</head>
<body>
{% include "header" %}
<div class="content">
    <a href="#" class="logo" style="background-image :url('{{ 'logo.svg' | asset_url | img_tag }}')"></a>
    {% include "alert_messages" %}
    {{ content_for_layout }}
    {% include "footer" %}
</div>
<!--[if lt IE 9]>
{{ 'es5-shim.min.js' | asset_url | script_tag }}
{{ 'html5shiv.min.js' | asset_url | script_tag }}
{{ 'html5shiv-printshiv.min.js' | asset_url | script_tag }}
{{ 'respond.min.js' | asset_url | script_tag }}
<![endif]-->
<script type='text/javascript' src='https://code.jquery.com/jquery-3.1.1.min.js'></script>
{{ 'core.js' | kajabi_asset_url | script_tag }}
{{ 'svg4everybody.min.js' | asset_url | script_tag }}
{{ 'bootstrap.min.js' | asset_url | script_tag }}
{{ 'responsify.min.js' | asset_url | script_tag }}
{{ 'scripts.js' | asset_url | script_tag }}
{{ 'main.js' | asset_url | script_tag }}
<script type="text/javascript" src="//platform-api.sharethis.com/js/sharethis.js#property=58a23b816e5792001278c642&product=inline-share-buttons"></script>
</div>
